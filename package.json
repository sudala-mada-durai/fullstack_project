{
  "name": "fullstack-todo-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "install": "npm install --workspaces",
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
    "build": "npm run build --workspaces"
  },
  "devDependencies": {
    "concurrently": "^7.0.0"
  }
}
